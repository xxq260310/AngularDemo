<!DOCTYPE html>
<html ng-app="MyAngularMVCApp" ng-controller="myListFactoryCtrl">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>{{query}}</title>
</head>
<body>
    <div class="panel panel-primary">
        <div class="panel-heading">
            <div class="panel-title">
                List
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-12">
                    <input type="text" placeholder="Search..." ng-model="query" class="navbar-text" />
                    <br />
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered">
                            <ngthead></ngthead>
                            <tr ng-repeat="user in employees">
                                <td>{{user.EmployeeId}}</td>
                                <td>{{user.FirstName}}</td>
                                <td>{{user.LastName}}</td>
                                <td>{{user.Country}}</td>
                                <td>{{user.State}}</td>
                                <td>{{user.IsActive}}</td>
                                <td>{{user.Description}}</td>
                                <td>{{user.BirthDate}}</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
